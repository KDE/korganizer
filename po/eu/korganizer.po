msgid "Scroll to Today"
msgstr "Kiribildu gaurko egunera"

#: actionmanager.cpp:469
#, kde-format
msgctxt "@info:tooltip"
msgid "Scroll the view to today"
msgstr "Kiribildu ikuspegia gaurko egunera"

#: actionmanager.cpp:470
#, kde-format
msgctxt "@info:whatsthis"
msgid "Scrolls the current view to the today's date."
msgstr "Kiribildu uneko ikuspegia gaurko datara."

#: actionmanager.cpp:476
#, kde-format
msgctxt "@action:inmenu scroll backward"
msgid "&Backward"
msgstr "A&tzerantz"

#: actionmanager.cpp:478
#, kde-format
msgctxt "@action:button scroll backward"
msgid "Back"
msgstr "Atzera"

#: actionmanager.cpp:479
#, kde-format
msgctxt "@info:status"
msgid "Scroll Backward"
msgstr "Kiribildu atzerantz"

#: actionmanager.cpp:480
#, kde-format
msgctxt "@info:tooltip"
msgid "Scroll the view backward"
msgstr "Kiribildu ikuspegia atzerantz"

#: actionmanager.cpp:482
#, kde-format
msgctxt "@info:whatsthis"
msgid ""
"Scrolls backward by a day, week, month or year, depending on the current "
"calendar view."
msgstr ""
"Kiribildu atzerantz, egun, aste, hilabete edo urte bat, uneko egutegi "
"ikuspegiaren arabera."

#: actionmanager.cpp:489
#, kde-format
msgctxt "@action:inmenu scroll forward"
msgid "&Forward"
msgstr "A&urrerantz"

#: actionmanager.cpp:491
#, kde-format
msgctxt "@action:button scroll forward"
msgid "Forward"
msgstr "Aurrerantz"

#: actionmanager.cpp:492
#, kde-format
msgctxt "@info:status"
msgid "Scroll Forward"
msgstr "Kiribildu aurrerantz"

#: actionmanager.cpp:493
#, kde-format
msgctxt "@info:tooltip"
msgid "Scroll the view forward"
msgstr "Kiribildu ikuspegia aurrerantz"

#: actionmanager.cpp:495
#, kde-format
msgctxt "@info:whatsthis"
msgid ""
"Scrolls forward by a day, week, month or year, depending on the current "
"calendar view."
msgstr ""
"Kiribildu aurrerantz, egun, aste, hilabete edo urte bat, uneko egutegi "
"ikuspegiaren arabera."

#: actionmanager.cpp:501
#, kde-format
msgctxt "@action:inmenu"
msgid "&Day"
msgstr "E&guna"

#: actionmanager.cpp:502
#, kde-format
