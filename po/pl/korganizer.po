#: actionmanager.cpp:535
#, kde-format
msgctxt "@info:status"
msgid "Scroll Forward"
msgstr "Przewiń wprzód"

#: actionmanager.cpp:536
#, kde-format
msgctxt "@info:tooltip"
msgid "Scroll the view forward"
msgstr "Przewiń widok naprzód"

#: actionmanager.cpp:538
#, kde-format
msgctxt "@info:whatsthis"
msgid ""
"Scrolls forward by a day, week, month or year, depending on the current "
"calendar view."
msgstr ""
"Przewija wprzód o dzień, tydzień, miesiąc lub rok, w zależności od bieżącego "
"widoku kalendarza."

