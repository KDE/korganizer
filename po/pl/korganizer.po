#: actionmanager.cpp:524
#, kde-format
msgctxt "@info:status"
msgid "Scroll Backward"
msgstr "Przewiń wstecz"

#: actionmanager.cpp:525
#, kde-format
msgctxt "@info:tooltip"
msgid "Scroll the view backward"
msgstr "Przewiń widok wstecz"

#: actionmanager.cpp:527
#, kde-format
msgctxt "@info:whatsthis"
msgid ""
"Scrolls backward by a day, week, month or year, depending on the current "
"calendar view."
msgstr ""
"Przewija wstecz o dzień, tydzień, miesiąc lub rok, w zależności od bieżącego "
"widoku kalendarza."

#: actionmanager.cpp:533
#, kde-format
msgctxt "scroll forward"
