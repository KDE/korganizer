#: actionmanager.cpp:516
#, kde-format
msgctxt "@info:tooltip"
msgid "Scroll the view to today"
msgstr "Przewiń widok do dzisiejszego dnia"

#: actionmanager.cpp:517
#, kde-format
msgctxt "@info:whatsthis"
msgid "Scrolls the current view to the today's date."
msgstr "Przewija bieżący widok do dzisiejszej daty."

#: actionmanager.cpp:522
#, kde-format
msgctxt "scroll backward"
