msgstr "오늘"

#: actionmanager.cpp:516
#, kde-format
msgctxt "@info:status"
msgid "Scroll to Today"
msgstr "오늘로 스크롤"

#: actionmanager.cpp:517
#, kde-format
msgctxt "@info:tooltip"
msgid "Scroll the view to today"
msgstr "보기를 오늘로 스크롤"

#: actionmanager.cpp:518
#, kde-format
